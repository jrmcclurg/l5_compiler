(
   (
      (x <- 10)
      (y <- 20)
      (z <- 30)
      (ecx <- x)
      (edx <- y)
      (eax <- z)
      (call :adder3)
      (r <- eax)
      (r *= 2)
      (r += 1)
      (eax <- (print r))
   )
   (:adder2
      (a <- ecx)
      (b <- edx)
      (t <- a)
      (t += b)

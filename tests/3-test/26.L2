   )
   (:adder3
      (a <- ecx)
      (b <- edx)
      (c <- eax)
      (ecx <- a)
      (edx <- b)
      (call :adder2)
      (x <- eax)
      (t <- x)
      (t += c)
      (eax <- t)
      (return)
   )

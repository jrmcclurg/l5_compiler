;; Simple L3 program which prints a triangle using integers
(
  ;; this is the program entry point
  (let ([maxVar 8])
  (let ([temp (:triangle 0 maxVar 1)]) temp))
  (:triangle (index max num)
     (let ([next (+ index 1)])
     (let ([c (= next max)])
     (let ([num2 (* num 10)])
     (let ([num3 (+ num2 next)])
     (if c (print num3)
           (let ([temp (print num3)])
           (:triangle next max num3) )))))))
)

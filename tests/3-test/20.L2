;; Simple L2 program which prints a triangle using integers
(
  ;; this is the program entry point
  (
    (maxVar <- 8)
    (ebx <- 1)

    (counterVar <- 1)
    :loop
    (cjump counterVar <= maxVar :continue :end)
    :continue
    (ebx *= 10)
    (ebx += counterVar)

    (eax <- ebx)
    (eax *= 2)
    (eax += 1)
    (eax <- (print eax))

    (counterVar += 1)
    (goto :loop)
    :end
  )
)
